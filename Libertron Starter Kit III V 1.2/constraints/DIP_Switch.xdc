# Libertron FPGA Starter Kit 3 V1.2 PIN MAP

# DIP Switch
# SW0 ~ SW7 VCCO 1.5 V

set_property -dict {PACKAGE_PIN J4 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[0]}]
set_property -dict {PACKAGE_PIN L3 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[1]}]
set_property -dict {PACKAGE_PIN K3 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[2]}]
set_property -dict {PACKAGE_PIN M2 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[3]}]
set_property -dict {PACKAGE_PIN K6 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[4]}]
set_property -dict {PACKAGE_PIN J6 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[5]}]
set_property -dict {PACKAGE_PIN L5 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[6]}]
set_property -dict {PACKAGE_PIN L4 IOSTANDARD LVCMOS15} [get_ports {DIP_SW[7]}]

# SW0 ~ SW7 VCCO 3.3 V

set_property -dict {PACKAGE_PIN Y16 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[8]}]
set_property -dict {PACKAGE_PIN AA16 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[9]}]
set_property -dict {PACKAGE_PIN AB16 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[10]}]
set_property -dict {PACKAGE_PIN AB17 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[11]}]
set_property -dict {PACKAGE_PIN AA13 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[12]}]
set_property -dict {PACKAGE_PIN AB13 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[13]}]
set_property -dict {PACKAGE_PIN AA15 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[14]}]
set_property -dict {PACKAGE_PIN AB15 IOSTANDARD LVCMOS33} [get_ports {DIP_SW[15]}]




